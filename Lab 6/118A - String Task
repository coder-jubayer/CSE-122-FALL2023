#include <iostream>
#include<string.h>
using namespace std;
int main()
{
    char x[100];
    cin>>x;
    int n;
    n=strlen(x);
    for(int i=0;i<n;i++){
        if(x[i]!='A'&& x[i]!='E'&& x[i]!='I'&& x[i]!='O'&&x[i]!='Y'&& x[i]!='U'&& x[i]!='a'&&x[i]!='e'&& x[i]!='i'&& x[i]!='o'&&x[i]!='u' && x[i]!='y'){
        x[i]=tolower(x[i]);
        cout<<".";
        cout<<x[i];
        }
    
    }
    
    return 0;
}
